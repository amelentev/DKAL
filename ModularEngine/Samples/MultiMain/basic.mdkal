// common
relation r(I: System.Int32)
---p1---
// principal p1 policy
do
	send to p2:
		r(1)
with X: System.Int32, Y: System.Int32
	upon
		p2 said r(X)
	if
		asInfon({|"basic"|Y:=X+1|})
	do
		send to p2:
			r(Y)
---p2---
// principal p2 policy
with X: System.Int32
	upon
		p1 said r(X)
	do
		send to p1:
			r(X)